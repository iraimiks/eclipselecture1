import java.util.Scanner;


public class HW32 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Please enter number");
		
		
		
		
		int triangleSize = scanner.nextInt();
		
		for(int i=1; i<= triangleSize; i++){
			System.out.println("");
			for(int j=1; j<=i; j++){
				System.out.print(i);
			}
			
		}
		

	}

}
