package com.accenture;

class Car{
	String color;
	int speed;
	String name;
	
	void drive(){
		System.out.println("drive name"+this.name);
	}
}

public class Repeating {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Car myCar = new Car();
		myCar.name = "MySuperDuperCar";
		myCar.drive();
		myCar.drive();
		myCar.drive();
		myCar.drive();
		myCar.drive();
		myCar.drive();
		
		
		Car stolenCar = new Car();
		stolenCar.name = "StolenCar";
		stolenCar.drive();
		

		myCar.drive();

		myCar.drive();

	}

}
