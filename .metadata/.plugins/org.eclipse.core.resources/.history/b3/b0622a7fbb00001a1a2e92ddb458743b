package com.accenture;

class Car{
	String color;
	int speed;
	String name;
	
	void drive(){
		System.out.println("drive");
	}
}

public class Repeating {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Car myCar = new Car();
		myCar.name = "MySuperDuperCar";
		myCar.drive();
		// TODO Auto-generated method stub

	}

}
