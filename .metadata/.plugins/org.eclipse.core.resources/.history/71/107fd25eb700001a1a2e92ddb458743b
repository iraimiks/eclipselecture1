package com.accenture;


class Bicycle{
	
	
	void direction(String side, int speedInKm){
		System.out.println("Side " + side + " speed: " + speedInKm + "km/h ");
	}
	
}

public class Arguments {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Bicycle bicycle = new Bicycle();
		String side = "North";
		int speed = 5;
		bicycle.direction(side, speed);

	}

}
